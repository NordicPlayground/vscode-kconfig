{
    "folding": {
        "markers": {
            "start": "^\\s*(choice|if|menu)",
            "end": "^\\s*(endchoice|endif|endmenu)"
        },
        "offSide": true
    },
    "comments": {
        "lineComment": "#"
    },
    "wordPattern": "[a-zA-Z0-9_\\-]+",
    "surroundingPairs": [
        [
            "(",
            ")"
        ],
        [
            "\"",
            "\""
        ]
    ],
    "brackets": [
        [
            "(",
            ")"
        ]
    ],
    "autoClosingPairs": [
        [
            "(",
            ")"
        ],
        [
            "\"",
            "\""
        ]
    ],
    "autoCloseBefore": ";:.,=}])>\n\t"
}
